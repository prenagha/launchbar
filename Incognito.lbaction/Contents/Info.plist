<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.renaghan.launchbar.Incognito</string>
	<key>CFBundleName</key>
	<string>Incognito Browser</string>
	<key>LBAbbreviation</key>
	<string>incognito private chrome www safari</string> 
	<key>CFBundleVersion</key>
	<string>3.2</string>
	<key>CFBundleIconFile</key>
  <string>font-awesome:user-secret</string>
  <key>LBARequiredApplication</key>
  <string>com.google.Chrome</string>
  <key>LBTextInputTitle</key>
  <string>URL</string>
	<key>LBDebugLogEnabled</key>
	<true/>
	<key>LBMinimumLaunchBarVersion</key>
	<integer>6121</integer>
	<key>LBScripts</key>
	<dict>
		<key>LBDefaultScript</key>
		<dict>
			<key>LBScriptName</key>
			<string>incognito.scpt</string>
      <key>LBKeepWindowActive</key>
      <true/>
			<key>LBRunInBackground</key>
			<false/>
			<key>LBRequiresArgument</key>
			<false/>
			<key>LBAcceptedArgumentTypes</key>
			<array>
			  <string>string</string>
			</array>
			<key>LBReturnsResult</key>
			<true/>
		</dict>
	</dict>
	<key>LBDescription</key>
	<dict>
		<key>LBSummary</key>
		<string>LaunchBar action that opens a Chromium browser window/tab in incognito private browsing mode. Opens to URL from active Safari tab, URL from clipboard, DuckDuckGo Search URL with clipboard term, or blank. If more than one option is available will show LaunchBar items for you to select.</string>
		<key>LBRequirements</key>
		<string>Chromium Browser https://github.com/Eloston/ungoogled-chromium</string>
		<key>LBAuthor</key>
		<string>Padraic Renaghan</string>
		<key>LBEmail</key>
		<string>prenagha@renaghan.com</string>
		<key>LBWebsiteURL</key>
		<string>https://renaghan.com/launchbar/incognito-browser/</string>
		<key>LBTwitter</key>
		<string>@prenagha</string>
		<key>LBArgument</key>
		<string>URL</string>
	  <key>LBUpdateURL</key>
	  <string>https://raw.githubusercontent.com/prenagha/launchbar/master/Incognito.lbaction/Contents/Info.plist</string>
	  <key>LBDownloadURL</key>
	  <string>https://minhaskamal.github.io/DownGit/#/home?url=https://github.com/prenagha/launchbar/tree/main/Incognito.lbaction</string>
	  <key>LBChangelog</key>
	  <string>
	    3.2: Fix download URL
	    3.0: Change to Chromium browser
	  </string>
	</dict>
</dict>
</plist>
