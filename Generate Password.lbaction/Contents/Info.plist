<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.renaghan.launchbar.GeneratePassword</string>
	<key>CFBundleName</key>
	<string>Generate Password</string>
	<key>CFBundleVersion</key>
	<string>6.2</string>
	<key>CFBundleIconFile</key>
	<string>font-awesome:fa-lock</string>
	<key>LBDebugLogEnabled</key>
	<true/>
	<key>LBScripts</key>
	<dict>
		<key>LBDefaultScript</key>
		<dict>
			<key>LBScriptName</key>
			<string>generate.js</string>
			<key>LBRunInBackground</key>
			<false/>
			<key>LBRequiresArgument</key>
			<false/>
			<key>LBReturnsResult</key>
			<true/>
		</dict>
	</dict>
	<key>LBDescription</key>
	<dict>
		<key>LBSummary</key>
		<string>Generate passwords, return results, and copy first to the clipboard. Uses https://github.com/bbusschots/hsxkpasswd for word passwords.</string>
		<key>LBAuthor</key>
		<string>Padraic Renaghan</string>
		<key>LBEmail</key>
		<string>prenagha@renaghan.com</string>
		<key>LBWebsiteURL</key>
		<string>https://renaghan.com/launchbar/generate-password/</string>
		<key>LBTwitter</key>
		<string>@prenagha</string>
	  <key>LBUpdateURL</key>
	  <string>https://raw.githubusercontent.com/prenagha/launchbar/master/Generate%20Password.lbaction/Contents/Info.plist</string>
	  <key>LBDownloadURL</key>
	  <string>https://minhaskamal.github.io/DownGit/#/home?url=https://github.com/prenagha/launchbar/tree/main/Generate%20Password.lbaction</string>
	  <key>LBChangelog</key>
	  <string>
	    6.2: Fix download URL
	    6.0: Returns multiple results, defaults to hsxkpasswd word based password
	    5.0: Rewritten in Javascript. Now supports preferences.
	  </string>
	</dict>
</dict>
</plist>
